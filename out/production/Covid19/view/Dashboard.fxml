<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="390.0" prefWidth="680.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DashBoardController">
   <children>
      <ImageView fitHeight="423.0" fitWidth="764.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/blue-and-green-coronavirus-infection-poster-vector.jpg" />
         </image>
      </ImageView>
      <Button fx:id="GlobalCOVID19" layoutX="232.0" layoutY="92.0" mnemonicParsing="false" onAction="#btnGlobalCOVID19_OnAction" prefHeight="45.0" prefWidth="289.0" text="Global COVID-19" textFill="#d53e19">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="ManageHospitals" layoutX="232.0" layoutY="156.0" mnemonicParsing="false" onAction="#btnMangeHospitals_ONAction" prefHeight="45.0" prefWidth="289.0" text="Manage Hospitals" textAlignment="CENTER" textFill="#18377a">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="QuarantineCenters" layoutX="233.0" layoutY="216.0" mnemonicParsing="false" onAction="#btnQuarantineCenters_OnAction" text="Manage Quarantine Centers">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="ManageUsers" layoutX="233.0" layoutY="275.0" mnemonicParsing="false" onAction="#btnManageUsers_OnAction" prefHeight="45.0" prefWidth="289.0" text="Manage Users" textFill="#df2828">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="lbl_userName" layoutX="227.0" layoutY="24.0" prefHeight="30.0" prefWidth="289.0" textAlignment="CENTER" textFill="#e3e310">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="Log_Out" layoutX="663.0" layoutY="320.0" mnemonicParsing="false" onAction="#btnLogout_OnAction" text="Log Out" textFill="#fe0000">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
