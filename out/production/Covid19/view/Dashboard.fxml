<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="395.0" prefWidth="764.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DashBoardController">
   <children>
      <ImageView fitHeight="402.0" fitWidth="764.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/blue-and-green-coronavirus-infection-poster-vector.jpg" />
         </image>
      </ImageView>
      <Button fx:id="GlobalCOVID19" layoutX="12.0" layoutY="12.0" mnemonicParsing="false" onAction="#btnGlobalCOVID19_OnAction" prefHeight="39.0" prefWidth="210.0" text="Global COVID-19" textFill="#d53e19">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="ManageHospitals" layoutX="560.0" layoutY="12.0" mnemonicParsing="false" onAction="#btnMangeHospitals_ONAction" text="Manage Hospitals" textAlignment="CENTER" textFill="#18377a">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="QuarantineCenters" layoutX="247.0" layoutY="12.0" mnemonicParsing="false" onAction="#btnQuarantineCenters_OnAction" text="Manage Quarantine Centers">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="ManageUsers" layoutX="605.0" layoutY="340.0" mnemonicParsing="false" onAction="#btnManageUsers_OnAction" prefHeight="45.0" prefWidth="146.0" text="Manage Users" textFill="#df2828">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
